@if(TempData["success"] != null)
{
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="~/lib/query/dist/jquery.min.js"></script>
    <script>
        Swal.fire(
            'Success',
            '@TempData["success"]',
            'success'
        )
    </script>
}

