@model List<LogsModel>

@{
    ViewData["Title"] = "Logs";
}

<div class="table-responsive">
    <table id="myTableNoSort" class="table table-hover" style="table-layout: fixed; width: 100%;">
        <colgroup>
            <col style="width: 10%"/>
            <col style="width: 100%"/>
            <col style="width: 20%"/>
        </colgroup>
        <thead>
            <tr>
                <th>Username</th>
                <th>Activity</th>
                <th>Date</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var logs in Model)
            {
                <tr>
                    <td>@logs.Username</td>
                    <td>@logs.Activity</td>
                    <td>@logs.Date</td>
                </tr>
            }
        </tbody>
    </table>
</div>

